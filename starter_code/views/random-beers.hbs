<div id="random">
  <img src="{{beer.image_url}}">
  <div class="text-beer">
    <h2>{{beer.name}}</h2>
    <h3>{{beer.tagline}}</h3>
    <p>{{beer.description}}</p>
    <h4>Food Pairing</h4>
    <ul>
      {{#each beer.food_pairing}}
      <li>{{this}}</li>
      {{/each}}
    </ul>
    <p><span>Brewers Tips:</span>{{beer.brewers_tips}}</p>
  </div>
</div>